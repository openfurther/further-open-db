/* SCHEMA_VERSION Table */
CREATE TABLE FRTHR_MODEL.SCHEMA_VERSION
( 
  VERSION_ID VARCHAR2(16),
  VERSION_DT DATE DEFAULT SYSDATE,
  CONSTRAINT SCHEMA_VERSION_PK PRIMARY KEY (VERSION_ID)
);
COMMENT ON TABLE FRTHR_MODEL.SCHEMA_VERSION 
        IS 'Tracks Database Schema or Data Model Version';
COMMENT ON COLUMN FRTHR_MODEL.SCHEMA_VERSION.VERSION_ID 
        IS 'Unique Schema Version ID or Number';
COMMENT ON COLUMN FRTHR_MODEL.SCHEMA_VERSION.VERSION_DT 
        IS 'Schema Version Datetime Defaults to Current DateTime';

