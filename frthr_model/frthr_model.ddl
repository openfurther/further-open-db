/* BEGIN CREATE TABLE */

CREATE TABLE FRTHR_MODEL.FPERSON
(
  FPERSON_ID NUMBER(*,0),
  DATASET_ID NUMBER(*,0),
  FPERSON_COMPOSITE_ID VARCHAR2(64),
  PEDIGREE_QUALITY NUMBER(*,0),
  CONSTRAINT FPERSON_PK PRIMARY KEY (FPERSON_ID, DATASET_ID) 
);
COMMENT ON TABLE FRTHR_MODEL.FPERSON IS 'Person';
COMMENT ON COLUMN FRTHR_MODEL.FPERSON.FPERSON_ID 
               IS 'FURTHER Person ID';
COMMENT ON COLUMN FRTHR_MODEL.FPERSON.DATASET_ID
               IS 'Dataset ID Similar to a FURTHER Query ID';
COMMENT ON COLUMN FRTHR_MODEL.FPERSON.FPERSON_COMPOSITE_ID 
               IS 'Composite key of dataset_id:fperson_id to reduce object-relational mapping complexity';
COMMENT ON COLUMN FRTHR_MODEL.FPERSON.PEDIGREE_QUALITY
               IS 'Kinship Coefficient in UPDB';

